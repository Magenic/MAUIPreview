<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Page A"
             x:Class="NavigationDemo.PageA">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Item 1" Order="Primary"></ToolbarItem>
        <ToolbarItem Text="Sub Item 2" Order="Secondary" Priority="1"></ToolbarItem>
        <ToolbarItem Text="Sub Item 1" Order="Secondary" Priority="0"></ToolbarItem>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Button Text="Page B" Clicked="PageB_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
